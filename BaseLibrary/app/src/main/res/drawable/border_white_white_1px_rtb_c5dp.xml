<?xml version="1.0" encoding="utf-8"?>
<!-- 白色边框 白色填充 右边上下圆角5dp圆角-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">  
    <item>  
        <shape>  
            <solid android:color="#ffffff" />  
            <stroke android:width="1px" android:color="#ffffff" />
            <corners android:topRightRadius="5dp" android:bottomRightRadius="5dp"/>  
        </shape>  
    </item>  
    <item android:top="1px" android:bottom="1px" android:left="1px" android:right="1px">  
        <shape>  
            <solid android:color="#ffffff" />  
            <corners android:topRightRadius="5dp" android:bottomRightRadius="5dp"/> 
        </shape>  
    </item>  
</layer-list>  
